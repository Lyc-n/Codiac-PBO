<TimerScreen>:
    name: "timer"
    
    MDFloatLayout:
        canvas.before:
            Color:
                rgba: 1,1,1,1
            Rectangle:
                source: "assets/bg1.png"
                pos: self.parent.pos
                size: self.parent.size
    MDBoxLayout:
        MDLabel:
            text: "Fitur Timer \n Belum Tersedia"
            pos_hint: {"center_x": 0.5,"center_y": 0.5}
            font_size: root.width/18
            font_name: "Poppins"
            bold: True
            size: self.size
            halign: "center"

    Navbar